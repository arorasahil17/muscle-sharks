{
    "builds":[
        {
            "src":"index.js",
            "use":"@vercel/node"
        },
        {
            "src":"dist/**",
            "use":"@vercel/static"
        }
    ],
    "routes":[
        {
            "src":"/api/(.*)",
            "dest":"index.js"
        },
        {
            "src":"/",
            "dest":"dist/index.html"
        },
        {
            "src":"/signin",
            "dest":"dist/index.html"
        },
        {
            "src":"/signup",
            "dest":"dist/index.html"
        },
        {
            "src":"/verification",
            "dest":"dist/index.html"
        },
        {
            "src":"/about",
            "dest":"dist/index.html"
        },
        {
            "src":"/contact",
            "dest":"dist/index.html"
        },
        {
            "src":"/cart",
            "dest":"dist/index.html"
        },
        {
            "src":"/user-orders",
            "dest":"dist/index.html"
        },
        {
            "src":"/products",
            "dest":"dist/index.html"
        },
        {
            "src":"/productDetails",
            "dest":"dist/index.html"
        },
        {
            "src":"/account",
            "dest":"dist/index.html"
        },
        {
            "src":"/change-password",
            "dest":"dist/index.html"
        },
        {
            "src":"/forgetPassword",
            "dest":"dist/index.html"
        },
        {
            "src":"/reset-password",
            "dest":"dist/index.html"
        },
        {
            "src":"/checkout",
            "dest":"dist/index.html"
        },
        {
            "src":"/pages/privacypolicy",
            "dest":"dist/index.html"
        },
        {
            "src":"/pages/return-and-refund-policy",
            "dest":"dist/index.html"
        },
        {
            "src":"/pages/terms-and-conditions",
            "dest":"dist/index.html"
        },
        {
            "src":"/*",
            "dest":"dist/index.html"
        },




        {
            "src":"/admin",
            "dest":"dist/index.html"
        },
        {
            "src":"/admin/login",
            "dest":"dist/index.html"
        },
        {
            "src":"/admin/cards",
            "dest":"dist/index.html"
        },
        {
            "src":"/admin/products",
            "dest":"dist/index.html"
        },
        {
            "src":"/admin/users",
            "dest":"dist/index.html"
        },
        {
            "src":"/admin/orders",
            "dest":"dist/index.html"
        },
        {
            "src":"/admin/coupon",
            "dest":"dist/index.html"
        },
        {
            "src":"/(.+)",
            "dest":"dist/$1"
        }
    ]
}